      "source": [
        "# League of Legends Match Predictor - Final Assignment\n",
        "\n",
        "# Step 1: Import libraries\n",
        "import torch\n",
        "import torch.nn as nn\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "from sklearn.metrics import confusion_matrix, roc_curve, auc, accuracy_score\n",
        "\n",
        "# Step 2: Create sample dataset (replace with real dataset if provided)\n",
        "np.random.seed(0)\n",
        "\n",
        "data = pd.DataFrame({\n",
        "    'kills': np.random.randint(0, 30, 200),\n",
        "    'deaths': np.random.randint(0, 30, 200),\n",
        "    'assists': np.random.randint(0, 30, 200),\n",
        "    'gold': np.random.randint(5000, 20000, 200),\n",
        "    'win': np.random.randint(0, 2, 200)\n",
        "})\n",
        "\n",
        "print(\"Dataset Preview:\")\n",
        "print(data.head())\n",
        "\n",
        "# Step 3: Preprocessing\n",
        "X = data.drop('win', axis=1)\n",
        "y = data['win']\n",
        "\n",
        "scaler = StandardScaler()\n",
        "X_scaled = scaler.fit_transform(X)\n",
        "\n",
        "X_train, X_test, y_train, y_test = train_test_split(\n",
        "    X_scaled, y, test_size=0.2, random_state=42)\n",
        "\n",
        "# Convert to tensor\n",
        "X_train = torch.FloatTensor(X_train)\n",
        "X_test = torch.FloatTensor(X_test)\n",
        "y_train = torch.FloatTensor(y_train.values).view(-1,1)\n",
        "y_test = torch.FloatTensor(y_test.values).view(-1,1)\n",
        "\n",
        "# Step 4: Define Logistic Regression Model\n",
        "class LogisticRegression(nn.Module):\n",
        "    def __init__(self, input_size):\n",
        "        super().__init__()\n",
        "        self.linear = nn.Linear(input_size,1)\n",
        "\n",
        "    def forward(self, x):\n",
        "        return torch.sigmoid(self.linear(x))\n",
        "\n",
        "model = LogisticRegression(X_train.shape[1])\n",
        "\n",
        "# Step 5: Loss and optimizer\n",
        "criterion = nn.BCELoss()\n",
        "optimizer = torch.optim.Adam(model.parameters(), lr=0.01)\n",
        "\n",
        "# Step 6: Training\n",
        "epochs = 100\n",
        "\n",
        "for epoch in range(epochs):\n",
        "    optimizer.zero_grad()\n",
        "    outputs = model(X_train)\n",
        "    loss = criterion(outputs, y_train)\n",
        "    loss.backward()\n",
        "    optimizer.step()\n",
        "\n",
        "print(\"Training Complete\")\n",
        "\n",
        "# Step 7: Evaluation\n",
        "with torch.no_grad():\n",
        "    predictions = model(X_test)\n",
        "    predicted = (predictions > 0.5).float()\n",
        "\n",
        "accuracy = accuracy_score(y_test, predicted)\n",
        "print(\"Accuracy:\", accuracy)\n",
        "\n",
        "# Step 8: Confusion Matrix\n",
        "cm = confusion_matrix(y_test, predicted)\n",
        "print(\"Confusion Matrix:\")\n",
        "print(cm)\n",
        "\n",
        "# Step 9: ROC Curve\n",
        "fpr, tpr, thresholds = roc_curve(y_test, predictions)\n",
        "roc_auc = auc(fpr, tpr)\n",
        "\n",
        "plt.figure()\n",
        "plt.plot(fpr, tpr)\n",
        "plt.title(\"ROC Curve\")\n",
        "plt.xlabel(\"False Positive Rate\")\n",
        "plt.ylabel(\"True Positive Rate\")\n",
        "plt.show()\n",
        "\n",
        "# Step 10: Save Model\n",
        "torch.save(model.state_dict(), \"model.pth\")\n",
        "\n",
        "# Step 11: Load Model\n",
        "model.load_state_dict(torch.load(\"model.pth\"))\n",
        "\n",
        "# Step 12: Hyperparameter tuning\n",
        "learning_rates = [0.1, 0.01, 0.001]\n",
        "best_acc = 0\n",
        "best_lr = 0\n",
        "\n",
        "for lr in learning_rates:\n",
        "    temp_model = LogisticRegression(X_train.shape[1])\n",
        "    optimizer = torch.optim.Adam(temp_model.parameters(), lr=lr)\n",
        "\n",
        "    for epoch in range(50):\n",
        "        optimizer.zero_grad()\n",
        "        outputs = temp_model(X_train)\n",
        "        loss = criterion(outputs, y_train)\n",
        "        loss.backward()\n",
        "        optimizer.step()\n",
        "\n",
        "    with torch.no_grad():\n",
        "        pred = temp_model(X_test)\n",
        "        pred = (pred > 0.5).float()\n",
        "        acc = accuracy_score(y_test, pred)\n",
        "\n",
        "    print(\"LR:\", lr, \"Accuracy:\", acc)\n",
        "\n",
        "    if acc > best_acc:\n",
        "        best_acc = acc\n",
        "        best_lr = lr\n",
        "\n",
        "print(\"Best Learning Rate:\", best_lr)\n",
        "\n",
        "# Step 13: Feature Importance\n",
        "importance = model.linear.weight.data.numpy()[0]\n",
        "\n",
        "features = X.columns\n",
        "\n",
        "plt.figure()\n",
        "plt.bar(features, importance)\n",
        "plt.title(\"Feature Importance\")\n",
        "plt.show()"
      ]
    }
  ]
